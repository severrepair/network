package Talk1;

public class ChatProtocol2 {
	
	//(C->S)ID:aaa
	//(S->C)CHALLIST:aaa;bbb;ccc;강호동;
	public static final String ID= "ID";
	
	//(C->S) CHAT:받는아이디;메세지 ex)CHAT:bbb;밥먹자
	//(S->C) CHAT:보내는아이디;메세지 ex)CHAT:aaa;밥먹자
	public static final String CHAT="CHAT";
	
	//(C->S)CHATALL:메세지
	//(S->C)CHATALL:[보낸아이디]메세지
	public static final String CHATALL="CHATALL";

	
	//(C->S) CHAT:받는아이디;쪽지내용 ex)MESSAGE:bbb;지금 어디?
	//(S->C) CHAT:보내는아이디;쪽지내용 ex)MESSAGE:aaa;지금 어디?
	public static final String MESSAGE = "MESSAGE";
	
	//(S->C) CHALLIST:aaa;bbb;ccc;강호동;
	public static final String CHATLIST = "CHATLIST";
	
	
	//구분자->프로토콜:data(delimiter)
	public static final String DM = ":";
	
}











