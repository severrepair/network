package ch14;

class MyThread extends Thread {
	
public class ThreadEx {
	String name;
	
	public MyThread(String name) {
		this.name=name;
	}
	@Override
	public void run() {
		try {
			for(int i=0;i<10;i++) {
				System.out.println(name + "i");
				Thread.sleep(500);	//0.5초 대기상태
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
	public static void main(String[] args) {
		

	}
	

}
